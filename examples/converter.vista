float goat = 100.0;
float bread = 2.0;
float iranian = 0.00002;
float iphone = 1000.0;
float cigarette = 20.0;

null quantity(string icon, float price, float currency) {
    print(icon + ":", round(currency/price,1));
}

float currencyInput(string prompt) {
    return float(input("Enter currency in GBP:"));
}

float currency = currencyInput("Enter Currency In GBP: ");


quantity("🚬",cigarette,currency);
quantity("🍞",bread,currency);
quantity("🐐",goat,currency);
quantity("📱",iphone,currency);
quantity("Iranian 💶",iranian,currency);